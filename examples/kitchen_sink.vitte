# Imports (alias + from-import)
import "math.vitte" as M
from "math.vitte" import double

# Closures + captures mutables
count = 0
inc = x -> count <- count + x
inc(2)

# Listes + map + range
lst = range(5)
lst.push(5)
each(lst, x -> print(x))

# Strings
a = "hello"
b = " world"
print(len(a))
a + b
"sum=" + 42   # one-literal concat via runtime

# Slices
sub = lst[1:4]
print(len(sub))
